{
  "routes": [
    {
      "src": "/",
      "has": [{ "type": "host", "value": "staking.cmem.ai" }],
      "dest": "/staking.html"
    },
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "has": [{ "type": "host", "value": "staking.cmem.ai" }],
      "dest": "/staking.html"
    },
    { "src": "/(.*)\\.html", "dest": "/$1" }
  ]
}
